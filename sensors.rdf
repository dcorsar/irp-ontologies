<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:user="http://www.w3id.org/abdn/irp/user/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dc="http://purl.org/dc/terms/"
    xmlns:spatialrelations="http://data.ordnancesurvey.co.uk/ontology/spatialrelations/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:devices="http://www.w3id.org/abdn/irp/devices/"
    xmlns="http://www.w3id.org/abdn/irp/sensors/"
    xmlns:ssn="http://purl.oclc.org/NET/ssnx/ssn#"
  xml:base="http://www.w3id.org/abdn/irp/sensors/">
  <owl:Ontology rdf:about="">
    <dc:title rdf:datatype="xsd:string">The IRP Sensors ontology</dc:title>
    <dc:date rdf:datatype="xsd:date">2014-11-12</dc:date>
    <dc:creator rdf:datatype="xsd:string">Milan Markovic</dc:creator>
    <dc:rights rdf:datatype="xsd:string">Licensed under the Lesser General Public Licence (LGPL) Version 2.1 (http://www.gnu.org/licenses/lgpl-2.1.html)</dc:rights>
    <rdfs:comment rdf:datatype="xsd:string">The IRP Sensors Ontology is a vocabulary for describing mobile devices and their (location) sensors.</rdfs:comment>
    <dc:creator rdf:datatype="xsd:string">David Corsar</dc:creator>
    <owl:imports rdf:resource="http://purl.oclc.org/NET/ssnx/ssn"/>
    <owl:versionInfo rdf:datatype="xsd:string">Revision: 1.0.1</owl:versionInfo>
    <dc:description rdf:datatype="xsd:string">The IRP Sensors Ontology is a vocabulary for describing mobile devices (Apple mobile phones and Android mobile phones) and their sensors.  The ontology was developed during the Informed Rural Passenger project at the dot.rural Digital Economy Hub, University of Aberdeen, UK uspported by the award made by the RCUK Digital Economy programme; award reference: EP/G066051/1.  The ontology is extends the Semantic Sensor Network ontology to support integration of sensor data from mobile devices with other sensor data.  Mobile devices are described as platforms on which a number of sensors (sensing devices) are deployed.</dc:description>
  </owl:Ontology>
  <owl:Class rdf:about="http://www.w3id.org/abdn/irp/sensors/AndroidLocationObservation">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Android location observation</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.w3id.org/abdn/irp/sensors/LocationDeviceObservation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://www.w3id.org/abdn/irp/sensors/iPhoneLocationDeviceSensorOutput">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >i phone location device sensor output</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.w3id.org/abdn/irp/sensors/LocationDeviceSensorOutput"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://www.w3id.org/abdn/irp/sensors/LocationDeviceObservation">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Location device observation</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >observationSamplingTime is the timestamp of the GPS</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.w3id.org/abdn/irp/sensors/LocationObservation"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#observationResult"/>
        <owl:allValuesFrom>
          <owl:Class rdf:about="http://www.w3id.org/abdn/irp/sensors/LocationDeviceSensorOutput"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >observationResultTime is the time the GPS was provided to the device</rdfs:comment>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >serverTimestamp is the time the observation was generated by the server (i.e. the data receive and processed)</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#observedBy"/>
        <owl:allValuesFrom>
          <owl:Class rdf:about="http://www.w3id.org/abdn/irp/sensors/LocationSensingDevice"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <skos:prefLabel rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Location device observation</skos:prefLabel>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="http://www.w3id.org/abdn/irp/sensors/serverTimestamp"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
